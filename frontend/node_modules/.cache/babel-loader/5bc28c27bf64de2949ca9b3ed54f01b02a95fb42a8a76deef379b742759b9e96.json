{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions, mapState } from \"vuex\";\nexport default {\n  name: \"SingleProduct\",\n  props: {\n    product: Object\n  },\n  data() {\n    return {\n      currProduct: null,\n      reviews: null\n    };\n  },\n  methods: {\n    ...mapActions([\"getReviewsForProduct\"])\n  },\n  computed: {\n    ...mapState([\"token\", \"reviewsForProduct\"])\n  },\n  mounted() {\n    console.log(this.$route.params.id);\n    this.getReviewsForProduct(this.$route.params.id).then(res => {\n      this.reviews = res;\n    });\n    let token = localStorage.token;\n    if (token === \"\") {\n      alert(\"NOT SIGNED IN\");\n      this.$router.push({\n        name: \"products\"\n      });\n      return;\n    }\n  }\n};","map":{"version":3,"mappings":";AAyBA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;IACA;MACA;IACA;IACA;IACA;MACAC;MACA;QAAAV;MAAA;MACA;IACA;EACA;AACA","names":["name","props","product","data","currProduct","reviews","methods","computed","mounted","console","alert"],"sourceRoot":"src/components","sources":["SingleProduct.vue"],"sourcesContent":["<template>\n  <div class=\"singleproduct\">\n    <h3>{{ product.product_name }}</h3>\n    <div v-if=\"product.image_url == 'slika'\" id=\"image\">IMAGE NOT AVALIABLE</div>\n    <ul>\n      <li>PRICE: {{ product.product_price }}</li>\n      <li>DESCRIPTION: {{ product.product_description }}</li>\n      <li>\n        Reviews:\n        <ul>\n          <li>Review Title: {{ reviews.review_title }}</li>\n          <li>Review: {{ reviews.review_text }}</li>\n        </ul>\n      </li>\n    </ul>\n    <div id=\"buttons\">\n      <b-button-group>\n        <b-button type=\"submit\" variant=\"dark\" id=\"submit\">Log In</b-button>\n        <b-button type=\"submit\" variant=\"dark\" id=\"goToSignIn\" to=\"/signup\">Go to Sign Up </b-button>\n      </b-button-group>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\nexport default {\n  name: \"SingleProduct\",\n  props: {\n    product: Object,\n  },\n  data() {\n    return {\n      currProduct: null,\n      reviews: null,\n    };\n  },\n  methods: {\n    ...mapActions([\"getReviewsForProduct\"]),\n  },\n  computed: {\n    ...mapState([\"token\", \"reviewsForProduct\"]),\n  },\n  mounted() {\n    console.log(this.$route.params.id);\n    this.getReviewsForProduct(this.$route.params.id).then((res) => {\n      this.reviews = res;\n    });\n    let token = localStorage.token;\n    if (token === \"\") {\n      alert(\"NOT SIGNED IN\");\n      this.$router.push({ name: \"products\" });\n      return;\n    }\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}